        public static int CompareCard(Card c1, Card c2)
        {
            if (c1.Suit > c2.Suit) return 1;
            if (c1.Suit < c2.Suit) return -1;
            if (c1.Rank == 0) return 1;
            if (c2.Rank == 0) return -1;
            if (c1.Rank > c2.Rank) return 1;
            if (c1.Rank < c2.Rank) return -1;
            return 0;
        }
