using System;

namespace Chapter10
{
	public class Program
	{
		public static void Main(string[] args)
		{
            Console.WriteLine(CanSpell("quijibo", "oui"));
            Console.WriteLine(CanSpell("Albert", "the"));
        }
        public static bool CanSpell(string letters, string possible)
        {

            int[] count = new int[26];
            for (int i = 0; i < possible.Length; i++)
            {
                char curChar = possible[i];
                if (letters.Contains(curChar) != true) return false;

            }
            return true;
        }
    }
}
