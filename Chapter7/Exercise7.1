using System;

namespace Math
{
    internal class Program
    {
        static void Main(string[] args)
        {
            Backwards("Hello World!");
        }
        public static void Backwards(string s)
        {
            for (int i = s.Length - 1; i >= 0; i--)
            {
                Console.Write(s[i]);
            }
        }
    }
}
