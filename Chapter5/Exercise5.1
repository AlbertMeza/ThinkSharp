using System;
namespace Chapter5
{
    internal class Program
    {
        static bool IsDivisible(int n, int m)
        {
            return (n % m == 0);
        }
        
        public static void Main(string[] args)
        {
            Console.WriteLine(IsDivisible(10,2));
            Console.WriteLine(IsDivisible(10,3));
        }
    }
}
