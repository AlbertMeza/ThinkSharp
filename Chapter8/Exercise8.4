using System;
using System.Numerics;

namespace Big
{
    internal class Program
    {
        public static void Main(string[] args)
        {
            for (int i = 0; i <= 150; i++)
            {
                Console.WriteLine(Factorial(i));
            }
        }

        public static BigInteger Factorial(int n)
        {
            if (n == 0)
            {
                return 1;
            }
            else
            {
                return n * Factorial(n - 1);
            }
        }
    }
}
