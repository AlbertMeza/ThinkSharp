using System;

namespace Chapter6
{
    internal class Program
    {

        static void Main(string[] args)
        {
            Console.WriteLine(Factorial(5));
            Console.WriteLine(Factorial(10));
        }
        public static int Factorial(int x)
        {
            int total = 1;
            while (x > 0)
            {
                total *= x;
                x--;
            }
            return total;
        }

    }
}
